<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.binbin.testas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FrameActivity"/>
        <activity android:name=".MyFrameActivity1"/>
        <activity android:name=".MyFrameActivity2"/>
        <activity android:name=".GifActivity"
                  android:hardwareAccelerated="false"/>
        <activity android:name=".SurfaceViewActivity"/>
        <activity android:name=".OpenGLActivity"
            android:hardwareAccelerated="true"/>
    </application>

</manifest>
